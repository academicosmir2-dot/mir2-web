<script>
function mostrarImagen(src){
  var visor = document.getElementById("visor");
  var img = document.getElementById("imgGrande");
  img.src = src;
  visor.style.display = "flex";
}
function cerrarVisor(){
  document.getElementById("visor").style.display = "none";
}
</script>

<button id="btnArriba" onclick="subirArriba()">‚Üë</button>




<script>
window.onscroll = function(){
  var btn = document.getElementById("btnArriba");
  if(document.documentElement.scrollTop > 200 || document.body.scrollTop > 200){
    btn.style.display = "block";
  } else {
    btn.style.display = "none";
  }
}

function subirArriba(){
  window.scrollTo({top:0, behavior:'smooth'});
}
</script>






<script>

async function buscarAlumno() {
  const dni = document.getElementById("dni").value.trim();
  const clave = document.getElementById("clave").value.trim();
  const resultado = document.getElementById("resultado");

  if(dni === "" || clave === ""){
    resultado.innerHTML = "Complete DNI y contrase√±a.";
    return;
  }

  const response = await fetch("resultados.csv");
  const data = await response.text();
  const filas = data.split("\n");

  let encontrado = false;

  for(let i = 1; i < filas.length; i++){
   const cols = filas[i].split(";");


    if(cols.length < 9) continue;

    const dniCSV = cols[0].trim();
    const nombre = cols[1];
    const grupo = cols[2];
    const s1 = cols[3] || "";
    const s2 = cols[4] || "";
    const s3 = cols[5] || "";
    const s4 = cols[6] || "";
    const s5 = cols[7] || "";
    const claveCSV = cols[8].trim();

    if(dni === dniCSV && clave === claveCSV){
      encontrado = true;
      resultado.innerHTML = `
        <h3>${nombre}</h3>
        <p><strong>Grupo:</strong> ${grupo}</p>
        <table border="1" width="100%" cellpadding="8">
          <tr><th>Semana</th><th>Nota</th></tr>
          <tr><td>1</td><td>${s1}</td></tr>
          <tr><td>2</td><td>${s2}</td></tr>
          <tr><td>3</td><td>${s3}</td></tr>
          <tr><td>4</td><td>${s4}</td></tr>
          <tr><td>5</td><td>${s5}</td></tr>
        </table>
      `;
      break;
    }
  }

  if(!encontrado){
    resultado.innerHTML = "DNI o contrase√±a incorrectos.";
  }
}

</script>




<section id="acceso">
  <div class="box">
    <h2>Consulta de Resultados</h2>
    <p>Ingrese su DNI y contrase√±a para ver sus simulacros.</p>

    <input type="text" id="dni" placeholder="DNI" style="padding:10px; width:100%; margin:5px 0;">
    <input type="password" id="clave" placeholder="Contrase√±a" style="padding:10px; width:100%; margin:5px 0;">
    <button onclick="buscarAlumno()" class="btn">Ingresar</button>

    <div id="resultado" style="margin-top:20px;"></div>
  </div>
</section>


